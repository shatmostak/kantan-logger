{
  "name": "kantan-logger",
  "version": "1.0.12",
  "description": "easy logger",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "easy",
    "logger",
    "kantan"
  ],
  "author": "Will Shostak <william.shostak@gmail.com> - Matt Shostak <matthewpshostak@gmail.com>",
  "license": "ISC",
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "sort-imports": [
        "error",
        {
          "ignoreMemberSort": true
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-plusplus": [
        "error",
        {
          "allowForLoopAfterthoughts": true
        }
      ]
    },
    "globals": {
      "logger": true,
      "kantan": true
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dateformat": "^3.0.3",
    "find-remove": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1"
  },
  "homepage": "https://github.com/shatmostak/kantan-logger#readme",
  "bugs": {
    "url": "https://github.com/shatmostak/kantan-logger/issues"
  }
}
